
module.exports = function removeFromProjectReferences(project, file) {
  var firstProject = project.getFirstProject().firstProject;

  var projectRef = firstProject.projectReferences.find(function (item) {
    return item.ProjectRef === file.uuid;
  });

  if (!projectRef) {
    return null;
  }

  firstProject.projectReferences.splice(firstProject.projectReferences.indexOf(projectRef), 1);

  return projectRef;
};